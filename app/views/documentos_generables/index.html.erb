<h1>Listing documentos_generables</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Modelo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @documentos_generables.each do |documento_generable| %>
  <tr>
    <td><%= documento_generable.nombre %></td>
    <td><%= documento_generable.modelo %></td>
    <td><%= link_to 'Show', documento_generable %></td>
    <td><%= link_to 'Edit', edit_documento_generable_path(documento_generable) %></td>
    <td><%= link_to 'Destroy', documento_generable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

