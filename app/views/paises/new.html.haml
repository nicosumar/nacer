- content_for :title do
  Paises

#content-header
  %h1 Paises
  #action-links
    = link_to "<< Volver ", :action => "index"

= render :partial => "shared/info", :locals => { :registro => @pais }

#content-body
  %h3 Agregar pais
  = form_for @pais do |p|
    = render(:partial => "form", :locals => {:p => p})
    %div.field_content
      = p.submit
