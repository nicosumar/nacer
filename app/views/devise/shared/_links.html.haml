- if controller_name != 'user_sessions'
  %div= link_to "Iniciar la sesión", new_session_path(resource_name)
- if devise_mapping.registerable? && controller_name != 'registrations'
  %div= link_to "Crear una cuenta", new_registration_path(resource_name)
- if devise_mapping.recoverable? && controller_name != 'passwords'
  %div= link_to "¿Olvidó su contraseña?", new_password_path(resource_name)
- if devise_mapping.confirmable? && controller_name != 'confirmations'
  %div= link_to "¿No recibió instrucciones de confirmación?", new_confirmation_path(resource_name)
- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  %div= link_to "¿No recibió instrucciones para desbloquear su cuenta?", new_unlock_path(resource_name)
