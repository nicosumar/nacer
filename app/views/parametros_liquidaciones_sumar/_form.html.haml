= form_for(@parametro_liquidacion_sumar) do |f|
  .field
    = f.label :dias_de_prestacion, "Tomar prestaciones hasta dias: "
    = f.number_field :dias_de_prestacion
  .field
    = f.label :formula, "Formula de calculo: "
    = f.select :formula_id, @formulas
  .field
    = f.label :nomenclador, "Nomenclador: "
    = f.select :nomenclador_id, @nomencladores
  .actions
    = f.submit

